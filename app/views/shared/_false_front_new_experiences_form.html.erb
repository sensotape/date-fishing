      <div class="create-form-flexbox">
        <i class="fas fa-angle-left left-side-arrow arrow-hide"></i>
        <div class="upper-form">
          <div class="form-group">
            <i class='fas fa-heart'></i>
            <i class="far fa-heart"></i>
           <i class="far fa-heart"></i>
            <h3>What's the name of your date?</h3>
            <input type="text" placeholder="Tell us your perfect date!" id="input-1">
          </div>
          <div class="form-group">
            <h3>And which category is it in?</h3>
            <select id="input-2" placeholder="Select One">
              <option id="intro-select" style="color:lightgrey;">Select One</option>
            <% Experience::CATEGORIES.each do |category| %>
              <option value="<%=category%>" style="color:red;"><%=category%></option>
            <% end %>
            </select>
          </div>
        </div>
        <!-- div class="button-container-form"> -->
          <!-- <div class="btn btn-primary muted"><</div> -->
          <i class="fas fa-angle-right right-side-arrow" id="new-date-1"></i>
        <!-- </div> -->
      </div>
    </div>

    <div class="formContainer">
      <div class="create-form-flexbox">
          <i class="fas fa-angle-left left-side-arrow" id="new-date-2-back"></i>
        <div class="upper-form">
          <div class="form-group">
            <i class='fas fa-heart'></i>
            <i class='fas fa-heart'></i>
           <i class="far fa-heart"></i>
            <h3>Add some photos!</h3>
            <div class="create-form-image-flexbox">
              <div class="btn btn-primary btn-special-photo" id="photos-button">+</div>
              <div id="photo-file-name"></div>
            </div>
          </div>
          <div class="form-group">
            <h3>And describe your date üòç</h3>
          <input type="text" placeholder="Whats the plan, Stan?" id="input-3">
          </div>
        </div>

          <i class="fas fa-angle-right right-side-arrow" id="new-date-2"></i>
      </div>
    </div>

    <div class="formContainer">

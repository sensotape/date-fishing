<%= simple_form_for @experience do |f| -%>

  <%= f.input :title, required: true, error: 'Must have between 4 and 50 characters' %>

  <%= f.input :category, required: true, collection: Experience::CATEGORIES %>

  <%= f.input :description, required: true %>

  <%= f.input :location %>

  <%= f.input :start_date %>

  <%= f.input :end_date %>


  <%= f.submit class: "btn btn-primary"%>

<% end %>

